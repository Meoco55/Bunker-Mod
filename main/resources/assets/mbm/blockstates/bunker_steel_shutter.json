{
    "multipart": [
        {   "apply": { "model": "mbm:bunker_steel_shutter_post" }},
        {   "when": { "north": true },
            "apply": { "model": "mbm:bunker_steel_shutter_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "mbm:bunker_steel_shutter_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "mbm:bunker_steel_shutter_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "mbm:bunker_steel_shutter_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "mbm:bunker_steel_shutter_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "mbm:bunker_steel_shutter_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "mbm:bunker_steel_shutter_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "mbm:bunker_steel_shutter_noside", "y": 270 }
        }
    ]
}
